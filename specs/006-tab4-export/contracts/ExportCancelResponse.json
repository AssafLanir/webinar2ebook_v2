{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExportCancelResponse",
  "description": "Response for POST /api/projects/{project_id}/ebook/export/cancel/{job_id}",
  "type": "object",
  "properties": {
    "data": {
      "oneOf": [
        {
          "type": "object",
          "properties": {
            "cancelled": {
              "type": "boolean",
              "description": "True if cancellation was successful"
            }
          },
          "required": ["cancelled"]
        },
        { "type": "null" }
      ]
    },
    "error": {
      "oneOf": [
        {
          "type": "object",
          "properties": {
            "code": {
              "type": "string",
              "enum": ["PROJECT_NOT_FOUND", "EXPORT_JOB_NOT_FOUND", "JOB_ALREADY_COMPLETED", "INTERNAL_ERROR"]
            },
            "message": {
              "type": "string"
            }
          },
          "required": ["code", "message"]
        },
        { "type": "null" }
      ]
    }
  },
  "required": ["data", "error"]
}
